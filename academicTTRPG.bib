@thesis{zhe__2022,
  type = {Master in Communication \& Technology},
  title = {探討 LARP 玩家遊戲表現、敘事傳輸和暫時擴張自我界限 對享樂感的影響 : The Effect of Gaming Performance, Narrative Transportation, Temporarily Expanding the Boundaries of the Self on the Enjoyment of LARP},
