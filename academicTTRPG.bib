@article{myers_simulating_1992,
  title = {Simulating the {{Self}}},
  author = {Myers, David},
  date = {1992},
  journaltitle = {Play \& Culture},
  volume = {5},
  number = {4},
  pages = {420--440},
  issn = {0894-4253},
  url = {https://psycnet.apa.org/record/1993-15727-001},
  abstract = {A structural analysis was conducted of the rules of some of the most popular role-playing games of the 1980s in order to discover how these games simulate human characters. Analysis is restricted to the rules of these games; the behavioral components of role playing are not considered. Thus correlations are drawn between games of various genres, and comments are made on the strategies of so-called universal role-playing systems. Final comments concern the recursive nature of play within role-playing games and the dialectic of play.},
