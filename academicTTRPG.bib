@thesis{mcneilly_dramaturgies_2021,
  type = {MA Drama},
  title = {Dramaturgies of {{Being}}: {{Agency}}, {{Pedagogy}} and the {{Self-Focused Performance}} of {{Tabletop RPGs}}},
