@article{abbott_table-top_2022,
  title = {Table-Top Role-Playing Games as a Therapeutic Intervention with Adults to Increase Social Connectedness},
  author = {Abbott, Matthew S. and Stauss, Kimberly A. and Burnett, Allen F.},
  date = {2022},
