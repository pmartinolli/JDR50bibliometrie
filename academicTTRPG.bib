  url = {https://books.google.ca/books?id=_d98DwAAQBAJ&pg=PT261&lpg=PT261&dq=%22Classical+Reception+and+the+Half-Elf+Cleric%22&source=bl&ots=e1IBRq4uge&sig=ACfU3U0l16WJ5OLbjdsrFGxtikV8g7ywFA&hl=en&sa=X&ved=2ahUKEwjUxMCL3e3gAhXpmOAKHVaYCfEQ6AEwAXoECAkQAQ#v=onepage&q=%22Classical%20Reception%20and%20the%20Half-Elf%20Cleric%22&f=false},
