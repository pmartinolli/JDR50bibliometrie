  booktitle = {Proceedings of the 18th {{International Conference}} on the {{Foundations}} of {{Digital Games}}},
  author = {Acharya, Devi and Kelly, Jack and Tate, William and Joslyn, Maxwell and Mateas, Michael and Wardrip-Fruin, Noah},
