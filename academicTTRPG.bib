@thesis{lees_tabletop_2022,
  type = {Ph.D. in Education},
  title = {Tabletop {{Therapy}}: {{An Examination}} of {{Therapeutic Factors Within Tabletop Role-Playing Game Groups}}},
  shorttitle = {Tabletop {{Therapy}}},
  author = {Lees, Oliver James},
  date = {2022},
  institution = {Indiana University},
  location = {Bloomington, IN},
  url = {https://www.proquest.com/docview/2736856245/abstract/5BC94726D6E140EBPQ/1},
  urldate = {2022-11-22},
  abstract = {There are a wide range of therapeutic factors that contribute to the growth and change of individuals taking part in group psychotherapy. Over time, many scholars have worked to describe, categorize, and measure these factors, to mixed success. All the while, similar types of groups have also been simultaneously yielding positive mental health benefits, while not receiving the same attention in the literature. One such type of group consists of those who gather for the purposes of playing tabletop role-playing games (TTRPGs). These games, which often involve imaginatively traversing planets, medieval kingdoms, or haunted forests, have been demonstrated to facilitate positive social and mental health outcomes in their players. Despite this, limited research has been done on the similarities between the two types of groups. Across two studies, I explored the specific factors of TTRPGs that players found to be helpful to their mental health. Responses were assessed using qualitative analysis and presented in the form of emergent themes, which were then subsequently compared with widely cited therapeutic factors of group therapy. Similarities and differences between group therapy and TTRPG groups are then discussed along with limitations of this study and directions for future research.},
  isbn = {9798357557582},
  langid = {english},
  pagetotal = {152},
  keywords = {_mental disorder,_PSYCHOLOGY,_therapy,_TTRPG,=cleaned}
}

