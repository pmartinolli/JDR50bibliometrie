@article{roberts_clinical_2015,
  title = {Clinical {{Interaction}} with {{Anthropomorphic Phenomenon}}: {{Notes}} for {{Health Professionals}} about {{Interacting}} with {{Clients Who Possess This Unusual Identity}}},
