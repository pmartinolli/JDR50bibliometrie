@inproceedings{mashny_dungeons_2023,
  title = {Dungeons \& {{Discourse}}: {{Tactical Social Media Research Methods}} and the {{D}}\&{{D Open Game License Case}}},
  shorttitle = {Dungeons \& {{Discourse}}},
  booktitle = {Proceedings of the 41st {{ACM International Conference}} on {{Design}} of {{Communication}}},
  author = {Mashny, Alex Michael and Strantz, Adam},
  date = {2023-10-26},
  series = {{{SIGDOC}} '23},
  pages = {191--196},
  publisher = {Association for Computing Machinery},
  location = {New York, NY, USA},
  doi = {10.1145/3615335.3623034},
  url = {https://dl.acm.org/doi/10.1145/3615335.3623034},
  urldate = {2023-11-02},
  abstract = {In December 2022, Wizards of the Coast announced that the next iteration of Dungeons and Dragons would discontinue the current Open Game License, which allows third-party creators and fans “permission to copy, modify and distribute” D\&D content. As technical communication researchers, we were fascinated by the online discussion around this event as news spread across various D\&D communities on the social media platform Reddit and even spilled out into broader roleplaying, gaming, and legal subreddits. Fans needed to reorient and resituate themselves across communities as developments unfolded, especially as moderators and prominent users strategically influenced the discourse within these subreddits. As both active members of these subreddits and researchers, we believe this case has much to teach the field of technical communication, particularly in the way it illustrates the need for tactical research methods when engaging with the shifting boundaries of social media research spaces. Researchers must also be savvy towards the limits of a platform, as well as their own positionality and their relationships to online communities when reporting on events. In reporting on the OGL case we aim to show how tactical research methods can help technical communication practitioners dynamically engage with social media platforms such as Reddit when studying events with widespread user engagement.},
  isbn = {9798400703362},
  keywords = {_copyright,_DESIGN,_dnd_game,_fandom,_TTRPG,=cleaned,Dungeons & Dragons,fan communities,Reddit,social media research methods,tactical technical communication}
}

