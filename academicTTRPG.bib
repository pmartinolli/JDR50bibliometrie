@article{clossen_dungeons_2024,
  title = {Dungeons and {{Dragons}} in the {{Information Literacy Classroom}}: {{How We Taught Bias}} in a {{For-Credit IL Class}}},
  shorttitle = {Dungeons and {{Dragons}} in the {{Information Literacy Classroom}}},
  author = {Clossen, Amanda},
