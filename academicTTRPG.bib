@article{lechner_psychodrama_2023,
  title = {Psychodrama und Drachen: Die Verwendung von Pen \& Paper-Rollenspielen in der Psychodrama-Psychotherapie mit Jugendlichen und jungen Erwachsenen},
  shorttitle = {Psychodrama and dragons: The use of pen-and-paper roleplaying games in psychodrama-psychotherapy with adolescents and young adults},
