  shorttitle = {Emotional {{Creativity}}},
  author = {Kuška, Martin and Trnka, Radek and Mana, Josef and Nikolai, Tomas},
  date = {2020-04-30},
  journaltitle = {Creativity Research Journal},
  volume = {32},
