@thesis{barrow_imaginative_2024,
  type = {MA Theatre},
  title = {Imaginative {{Immersion}}: {{Developing}} a {{Theatre}} of the {{Mind Pedagogy}} for an {{Ever-Changing Educational Landscape}}},
  shorttitle = {Imaginative {{Immersion}}},
  author = {Barrow, Cory Kennedy},
  date = {2024},
  institution = {University of Central Florida},
  location = {Orlando},
  url = {https://stars.library.ucf.edu/etd2023/106},
  abstract = {The suspension of disbelief is integral to the performing arts. We ask our actors to see couches made of three chairs, designers to create cities in empty spaces, and most of all; we ask audiences to believe the stories and relationships that are figurative and often abstract. This level of critical and creative engagement is assumed to develop in spaces of higher education. However, with an ever-changing world and increasing conversions and integrations of Artificial Intelligence (AI) in spaces of higher education, it has become even more apparent that students need to develop their creative and critical thinking skills earlier on in their development. By recontextualizing learning through imaginative immersive game systems, such as Dungeons \& Dragons and similar tabletop role-playing games (TTRPGs), I have found that students experience categories of significant learning synergistically through experiential gameplay. This immersive storytelling relies entirely on the player-performers to spend their disbelief and immerse themselves in their imagination, termed theatre of the mind. The gamification of role- play acts as a means of challenging students to activate their development of collaborative improv, storytelling/playwriting, and dramaturgy skills. Through research and play testing, I have developed a gamified drama-focused role-play educational tool utilizing many of the core gameplay mechanics in the world’s most popular TTRPGs. In this thesis, I present the game structure and reflect upon my experiences implementing this work. Additionally, I explore the cross sections between game design and immersive theatre practice and consider how this intersection is utilized in gamified performance and pedagogy. Finally, I consider how a theatre- of-the-mind-based pedagogy can be utilized in subjects outside of the theatre classroom.},
  langid = {english},
  pagetotal = {109},
  keywords = {_classroom rpg,_dnd_game,_immersion,_improvisation,_LEARNING,_theater,_TTRPG,=cleaned}
}

