@article{caira_jeux_2014,
  title = {Jeux vidéo et jeux d’interaction en face-à-face : vers un modèle unifié d’écologie de l’intrigue},
  shorttitle = {Jeux vidéo et jeux d’interaction en face-à-face},
  author = {Caïra, Olivier},
  date = {2014},
  journaltitle = {Cahiers de Narratologie},
  number = {27},
  issn = {0993-8516},
  doi = {10.4000/narratologie.7058},
  url = {http://narratologie.revues.org/7058},
  urldate = {2015-05-20},
  abstract = {Le débat contemporain sur l'interactivité est largement dominé par le numérique, qu'il se présente sous la forme du récit numérique ou sous celle du jeu vidéo. Minoritaires dans les usages, rarement fixées sous la forme d'œuvres, économiquement insignifiantes, les pratiques narratives de face-à-face (jeux de rôle sur table et grandeur nature, «~storygames~») demeurent souvent cantonnées aux notes de bas de page et aux rappels historiques qui cadrent la discussion sans l'alimenter. J'entends rappeler l'intérêt que revêt une analyse plus équilibrée de l'interactivité, et notamment souligner l'inventivité des auteurs «~papier-crayon~», à l'heure où les techniques de scénarisation en jeu vidéo peinent à se renouveler. L'exemple de quelques storygames montrera comment l'improvisation collective en langue naturelle peut enrichir le regard des narratologues sur l'interactivité. L'idée n'est pas de «~jouer~» l'analogique «~contre~» le numérique, mais de bâtir un modèle unifié autour de leurs points communs~: les notions d'environnement et d'intrigue.},
  issue = {27},
  langid = {french},
  keywords = {_literature,_peer reviewed,_PSYCHOLOGY,_TTRPG,=0.66,=biblio:DNDP2018,=cleaned,=french,=p0,=PM-lu,=rabbit,écologie de l'intrigue,environnement ludique,fiction analogique,scénarisation},
  annotation = {QID: Q114731366}
}

@article{caira_jeux_2019,
  title = {Jeux de rôle sur table : une création de monde à trois niveaux},
  shorttitle = {Tabletop Role-Playing Games: A Creation of a World on Three Levels},
  author = {Caïra, Olivier},
  date = {2019-10-25},
  journaltitle = {Revue de la BNF},
  volume = {59},
  number = {2},
  pages = {89--95},
  publisher = {BNF},
  issn = {1254-7700},
  doi = {10.3917/rbnf.059.0089},
  url = {https://www.cairn-int.info/revue-revue-de-la-bibliotheque-nationale-de-france-2019-2-page-89.htm},
  urldate = {2021-05-07},
  abstract = {Nés dans les années 1970 avec Donjons et dragons, les jeux de rôle sur table proposent de vivre des aventures de fiction en y interprétant toutes sortes de personnages. Au contraire des jeux vidéo, où le monde est entièrement programmé par les développeurs, ces jeux favorisent une création à différents niveaux et par différents participants, de la proposition globale de l’auteur aux aménagements locaux des joueurs au fil de l’improvisation. « Vous êtes dans une auberge. Tout à coup, une femme aux cheveux roussis fait irruption parmi les buveurs en criant : “À l’aide ! Le temple est en flammes !” Que faites-vous ? » Une situation tendue et un « Que faites-vous ? » peuvent suffire à lancer un jeu de rôle sur table. Un des participants – nous venons de le faire parler – décrit les situations, interprète les personnages secondaires et arbitre les actions : c’est le meneur de jeu. Les autres créent et interprètent les héros d’une intrigue qui va se nouer et se dénouer au fil du dialogue et des jets de dés : ce sont les joueurs. Certains de ces « rôlistes » peuvent improviser durant des heures en imaginant simplement le décor et les protagonistes autour de leur personnage au fil de l’action. Mais ils choisissent pour la plupart de jouer leur personnage dans un monde qui préexiste à leurs parties et dans lequel d’autres aventures pourront se dérouler. Ces mondes présentent la particularité d’être construits sur au moins trois niveaux. On y accède d’abord par les ressources encyclopédiques fournies dans le manuel du jeu, qui constitue une sorte de « bible » créative pour ses utilisateurs…},
  langid = {french},
  keywords = {_literature,_metagame,_peer reviewed,_TTRPG,_worldbuilding,=cleaned},
  annotation = {QID: Q114772201}
}

@book{caira_jouer_2009,
  title = {Jouer avec l'Histoire},
  editor = {Caïra, Olivier and Larré, Jérôme},
  date = {2009},
  series = {L'atelier du jeu de rôle},
  edition = {1},
  publisher = {Pinkerton Press},
  location = {Villecresnes},
  url = {http://www.legrog.org/jeux/generique-historique/jouer-avec-l-histoire-fr},
  abstract = {Cet ouvrage est un recueil d'articles francophones sur le jeu de rôle et sa pratique, englobant des conseils de création ou de maîtrise. Il appartient à la collection "L'Atelier du Jeu de Rôle", qui se décline sur divers thèmes. Pour ce premier ouvrage de la collection, le thème exploré est l'Histoire. L'ouvrage, qui débute par une introduction d'Olivier Caïra ("Les Affleurements de l'Histoire", 4 pages) est divisé en trois parties contenant chacune trois articles. Trois jeux, trois traitements de l'Histoire     "Te Deum pour un massacre - Jouer dans une Histoire douloureuse" (14 pages) de Jean-Philippe Jaworski présente la genèse et les options ayant présidé à la création de ce jeu de rôle historique.     "Pavillon Noir - La piraterie entre Histoire et mythe" (16 pages) de Renaud Maroy fait de même pour Pavillon Noir.     "Maléfices - Odeur de soufre sur l'École des chartes" (14 pages) de Daniel Dugourd est consacré au jeu Maléfices. Jouer dans un cadre historique     "Comment écrire une campagne historique" (24 pages) de Jérôme Larré propose de nombreux conseils sur la manière de créer une campagne historique (dans notre réalité ou dans un jeu à l'histoire touffue), l'article est illustré par une des campagnes maison de l'auteur se passant au Japon.     "Traiter l'Histoire au travers du genre" (16 pages) de Romain d'Huissier est une réflexion sur l'utilisation du genre (polar, capes et d'épées, etc.) afin de typer un jeu historique et permettre, grâce à cette lentille, une meilleure immersion des joueurs.     "Les Rôlistes et le Moyen-Age : le familier et l'exotique" (20 pages) de Antoine Dauphragne présente les résultats d'une recherche universitaire sur les représentations du Moyen-Age dans la population roliste. Cette enquête est basée sur des entretiens de type qualitatifs menés auprès de rôlistes parisiens. Les plaies vives de l'Histoire     "Traiter les problématiques et les thèmes difficiles" (22 pages) de Christophe Chaudier et Yann Lefebvre discute, pour le jeu Crimes, de la manière de traiter des sujets délicats et adultes. Il propose des pistes de réflexion afin de mener et de créer des scénarios pour ce jeu.     "Nazisme et jeu de rôle" (12 pages) d'Olivier Caïra analyse la manière dont les jeux de rôle ont traité et présenté le thème délicat du Nazisme.     "Tentacules et croix gammées" (12 pages) de Christophe Valla présent la manière dont l'auteur de l'article, au travers du Pulp, a créé une campagne se déroulant durant la Seconde Guerre Mondiale. De nombreux encarts parsèment l'ouvrage et proposent des réflexions complémentaires ou des zooms sur des jeux particuliers. Cette fiche du GROG a été rédigée le 10 mai 2009.  Dernière mise à jour le 3 octobre 2009.},
  isbn = {978-2-9533916-0-2},
  langid = {french},
  pagetotal = {160},
  keywords = {_adaptation,_advice,_call of cthulhu_game,_crimes_game,_history,_major publication,_malefices_game,_MEDIA,_neomedievalism,_pavillonnoir_game,_tedeum_game,_TTRPG,=cleaned,=french}
}

