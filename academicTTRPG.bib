@inproceedings{tchernavskij_readymades_2022,
  title = {Readymades \& {{Repertoires}}: {{Artifact-Mediated Improvisation}} in {{Tabletop Role-Playing Games}}},
  shorttitle = {Readymades \& {{Repertoires}}},
  booktitle = {Creativity and {{Cognition}}},
  author = {Tchernavskij, Philip and Webb, Andrew M. and Gemeinhardt, Hayden and Mackay, Wendy E.},
  date = {2022-06-20},
  series = {C\&{{C}} '22},
  pages = {298--311},
