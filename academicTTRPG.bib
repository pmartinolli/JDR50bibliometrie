@article{josefsen_visions_2024,
  title = {Visions of Degeneracy: {{Race}}, Gender and Sexuality in '{{Exalted}}' Sourcebooks},
  shorttitle = {Visions of Degeneracy},
  author = {Josefsen, Niels-Martin Ström and Burnett, Scott},
  date = {2024-08-23},
  journaltitle = {Gender and Language},
