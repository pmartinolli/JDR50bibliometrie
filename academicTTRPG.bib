@thesis{rhodes_history_2024,
  type = {BA Honor},
  title = {The {{History}} of {{Verités}} and {{Her Peoples}}},
  author = {Rhodes, Declan},
  date = {2024-12-01},
  institution = {Arizona State University},
  url = {https://hdl.handle.net/2286/R.2.N.196261},
  urldate = {2024-09-06},
  abstract = {This thesis is the culmination of little ideas from nearly a decade of my life, all coming together to form a larger, cohesive world in which I hope to create a great many enthralling campaigns of Dungeons \& Dragons. I was inspired to create this work by a class which essentially kicked me in the pants to begin writing just a piece of history too far back for anyone to remember. This class, “Worldbuilding in Science Fiction and Fantasy” taught by professor Matt Bell, led me to create the beginning of a world which I had been toying with slowly for many years prior. At the start of the class, I had a map of one continent, a loose idea of the political workings of that continent, and several landmarks with unexplained and mysterious histories. By the end, I had created two more continents, an explanation for some of the landmarks, and many many more threads to toy with. The Complete History of Verités and Her Peoples is intended to be a piece of writing within the world of Verités, kept in the highly guarded personal library of a lich by the name of Visher Delcanis. This library will very likely be a location for players in my world to seek out, as the information contained within all holds immense power. It is my hope that players will face a threat so outlandish or powerful that they will have to turn to these forgotten histories to find a solution, or perhaps even just understand what the threat they are facing is.},
  langid = {english},
  pagetotal = {24},
  keywords = {_DESIGN,_designing rpg,_dnd_game,_TTRPG,=cleaned}
}

@article{ricci_live-action_2022,
