@thesis{sarturi_quando_2012,
  type = {Master’s Degree in Theater},
  title = {Quando os dados (não) rolam: jogo, teatralidade e performatividade na interação entre o roleplaying game e o Process Drama},
  shorttitle = {When the dice (don’t) roll: Game, Theatricality and Performativity in  the interaction between  ROLEPLAYING GAME and the Process Drama},
