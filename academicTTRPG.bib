@thesis{jeffries_exploratory_2019,
  type = {PhD Psychology},
  title = {An {{Exploratory Study}} of {{Individuals Who Play Fantasy Tabletop Role-Playing Games}}: {{A Heuristic Exploration}}},
  shorttitle = {An {{Exploratory Study}} of {{Individuals Who Play Fantasy Tabletop Role-Playing Games}}},
  author = {Jeffries, Pualani R.},
  date = {2019},
  institution = {Ashford University},
  abstract = {The purpose of this study was to understand the lived experiences of individuals who actively play fantasy tabletop role-playing games (TTRPGs). There is limited research on the lived gaming experience of individuals who play fantasy tabletop role-playing games in live gameplay. Fittingly, the current study utilized a qualitative heuristic research design based on Moustakas’s Heuristic Research Design, Methodology, and Application (1990). This study included five participants (referred to as co-researchers) over the age of 21 who actively participate in fantasy tabletop role-playing games (TTRPGs) or who have participated in TTRPGs, such as Dungeons and Dragons (D\&D), within the last year of the study. Five primary themes were identified that spoke to the collective experience of playing TTRPGs: (a) introduction to TTRPGs and first time playing TTRPGs, (b) participants play TTRPGs because it is social, (c) participants get personal benefits from playing TTRPGs, (d) playing these types of games is a large commitment for participants, and (e) creating stories together. The data gathered in the study were examined with previously completed research in the area of TTRPGs and role-playing games. This study identified and discussed limitations such as researcher bias, time dedicated to a qualitative study, and the total number of participants. Recommendations for future research include exploring levels of experience of TTRPGs players, generalizability with gender, and a suggestion for a mixed-method study to explore further the areas that are limited in qualitative studies.},
  langid = {english},
  pagetotal = {136},
  keywords = {_assessment,_dnd_game,_PSYCHOLOGY,_TTRPG,=cleaned}
}

