@thesis{elequin_dungeon_2020,
  type = {Thesis},
  title = {A {{Dungeon Master}}'s {{Guide}} to the {{Satanic Panic}}: {{The History}}, {{Sociology}}, and {{Rhetoric}} of {{Conservative Christianity}} and {{Dungeons}} and {{Dragons}}},
