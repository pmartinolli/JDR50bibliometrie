@inproceedings{fox_critical_2017,
  title = {Critical {{Roleplaying}}: {{An Exploration}} of {{Tabletop Role-Playing Games}} as {{Collaborative Fiction}} and the {{Roles}} of {{Players}} as {{Co-Authors}} of a {{Complex Narrative}}},
  author = {Fox, Rachelle},
  date = {2017-10},
  url = {https://librexandria.wordpress.com/2022/03/18/fcea-2017-cea-2018/},
  abstract = {To better understand the phenomenon that is Critical Role, and to unravel how it and similar live-streamed RPGs have evolved as a form of entertainment media intended not just for players, but for an audience, I looked at several factors, including the nature of TTRPGs as collaborative storytelling and the relationship of live-streamed gameplay to other forms of entertainment, such as improv shows, sports broadcasts, and reality television. When examined all together, it becomes clear that there is something more to this entertainment medium than simply watching people play a game, and that the appeal of Critical Role and other live-streamed TTRPG programs owes much to the medium’s ability to leverage the effect of narrative transportation—that feeling of being drawn into a narrative through imagination and empathy for the characters—as well as the intensely collaborative nature of this form of storytelling, which requires participants to make and commit to highly consequential story-changing decisions on the fly. The willingness of players to commit to the “yes, and” of improv transforms the role-playing game into a powerful vehicle for collaborative storytelling, while the unpredictability of real-time dice rolls combined with the inversion of traditional pacing (such that fights that would take only seconds in reality play out over the course of hours) keep viewers and players alike on the edge of their seats for hours at a time, wondering what will happen next.},
  eventtitle = {Critical {{Role}} at the {{Florida College English Association}}’s Annual Conference in 2017 \& the National {{College English Association}}’s Annual Conference in 2018},
  langid = {english},
  keywords = {_actual play,_MEDIA,_TTRPG,=cleaned}
}

@thesis{fraga_livros_2022,
  type = {Mestre em Filosofia pelo Programa de Pós-Graduação em Estudos Culturais},
  title = {Livros e revistas para jogar RPG (1980-2000): da importação às primeiras edições nacionais},
  shorttitle = {Livros e revistas para jogar RPG (1980-2000)},
  author = {Fraga, André Marques},
  date = {2022-06-06},
  institution = {Universidade de São Paulo},
  location = {São Paulo},
  doi = {10.11606/D.100.2022.tde-01082022-163244},
  url = {https://www.teses.usp.br/teses/disponiveis/100/100135/tde-01082022-163244/},
  urldate = {2024-05-28},
  abstract = {Esta dissertação tem por objeto os jogos de RPG, primordialmente em São Paulo, desde as primeiras menções em jornais, por volta da década de 1980, até a consolidação das primeiras coleções publicadas por editoras brasileiras, na década de 1990. Inicialmente trazidos ao Brasil por meio de jogadores que os adquiriram no exterior e os compartilharam por meio de fotocópias em xérox com suas comunidades, livros e revistas de RPG gradualmente atraíram o interesse de editoras nacionais, que passaram a publicá-los em edições traduzidas ou em versões originais, competindo por um lugar nesse mercado até meados dos anos 1990, período em que se dá o predomínio das editoras paulistas Devir e Trama. Partindo do princípio que o livro é central nesse gênero de jogos, utilizamos como aporte teórico e metodológico aqueles utilizados por historiadores do livro e da leitura, com a finalidade de compreender o processo pelo qual os livros de regras e os chamados suplementos de RPG instauraram-se em meio ao mercado editorial paulista, popularizando-se e expandindo-se em meio a eventos e comunidades locais de jogadores. Para compreender também, mais especificamente, questões voltadas à leitura e à apropriação e ressignificação dos jogos de RPG, vindos dos EUA ao Brasil, foram pesquisadas diversas edições da maior revista nacional especializada nestes jogos, a Dragão Brasil, analisando sua seção de cartas de leitores e alguns de seus conteúdos e ilustrações. Dessa maneira, procuramos apresentar as linhas gerais do que foi esse mercado, a partir do caso de São Paulo, nesses anos iniciais, e apontar elementos sobre como o gênero dos livros de RPG, em um primeiro momento, de forma americana, foi recriado no Brasil no período exposto.},
  langid = {portuguese},
  pagetotal = {205},
  keywords = {_brazil,_HISTORYOFRPG,_MEDIA,_publishing,_TTRPG,=cleaned}
}

