@article{leonhardt_recovery-oriented_2024,
  title = {A {{Recovery-Oriented Approach}}: {{Application}} of {{Metacognitive Reflection}} and {{Insight Therapy}} ({{MERIT}}) for {{Youth}} with {{Clinical High Risk}} ({{CHR}}) for {{Psychosis}}},
  shorttitle = {A {{Recovery-Oriented Approach}}},
  author = {Leonhardt, Bethany L. and Visco, Andrew C. and Hamm, Jay A. and Vohs, Jenifer L.},
  date = {2024-04},
  journaltitle = {Behavioral Sciences},
  volume = {14},
  number = {4},
  pages = {1--17},
  publisher = {Multidisciplinary Digital Publishing Institute},
  issn = {2076-328X},
  doi = {10.3390/bs14040325},
  url = {https://www.mdpi.com/2076-328X/14/4/325},
  urldate = {2024-04-19},
  abstract = {Clinical High Risk for psychosis (CHR) refers to a phase of heightened risk for developing overt psychosis. CHR often emerges during adolescence or early adulthood. CHR has been identified as a group to target for intervention, with the hope that early intervention can both stave off prolonged suffering and intervene before mental health challenges become part of an individual’s identity. However, there are few treatment modalities that can address some of the specific needs of CHR. Metacognitive Reflection and Insight Therapy (MERIT) is an integrative psychotherapy that can be applied to the CHR population. MERIT offers unique advantages to working with the CHR population as it aims to improve self-direction and recovery through stimulation of metacognitive capacity, a phenomenon that has been associated with recovery. This paper explores unique aspects of the CHR population and how MERIT can address barriers to recovery for individuals experiencing psychosis-like symptoms. Several case examples and a clinical vignette using MERIT to support patients with CHR are offered to exemplify this approach. MERIT offers a way to assist persons with CHR to address barriers to their personal recovery and to develop nuanced understandings of ways to master challenges.},
  issue = {4},
  langid = {english},
  keywords = {_peer reviewed,_PSYCHOLOGY,_therapy,_TTRPG,=0.33,=cleaned,clinical high risk,metacognition,psychosis,psychotherapy,recovery}
}

