@book{kuntz_4th_2024,
  title = {The 4th {{Category}}},
  author = {Kuntz, Robert J.},
  date = {2024},
  publisher = {Three Line Studio},
  abstract = {The 4th Category is a major update on Rob Kuntz's seminal treatise Dave Arneson's True Genius, as well as a preview to his upcoming research opus, A New Ethos in Game Design. This ground-breaking study which identifies D\&D's true origins includes historical matter concerning the rise of classic D\&D from its pre-LGTSA state under Dave Arneson, to the LGTSA play-tests, to its publication in 1974 and immediate post-publication phase from 1974-75. After Arneson emphatically captured Gary’s and the LGTSA's attention with his Blackmoor demo game, it was our turn to fully experience this new system. Several weeks into the new play-tests of the game system Gary, post-game, made an observation that I’ve previously referred to in the preface and is worth noting again: “We’ve created a fourth category in games.” He said this while looking at an indistinct spot in the dining room. He then went on to list the three categories of games that existed at the time… Table-Top Games… Parlor Games… Physical and Outdoor Activity Games… — excerpted from the Introduction to A New Ethos in Game Design. This work describes how Rob Kuntz went on to corroborate Gary Gygax’s monumental assertion. In it he presents the following proofs gleaned from his 16 years of studies and research:     The new system's qualities are meticulously explained     Its unique system architecture is revealed and its operations are explained in detail     Alongside this, Rob asserts that with D\&D's original publication, a breakout medium in storytelling — real time story crafting — was created, which corresponds in major ways to traditional oral storytelling.},
  langid = {english},
  pagetotal = {41},
  keywords = {_arneson,_blackmoor_game,_HISTORYOFRPG,_TTRPG,=cleaned},
  annotation = {https://threelinestudiostore.company.site/THE-4TH-CATEGORY-p628165080}
}

