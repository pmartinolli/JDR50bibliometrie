@thesis{thompson_when_2013,
  type = {MS Faculty of Social and Behavioural Sciences},
  title = {When {{Seeing}} Is {{Believing}}: {{The Construction}} of a {{Secondary World}} in {{Live Action Role Play}}},
  shorttitle = {When {{Seeing}} Is {{Believing}}},
  author = {Thompson, R.},
  date = {2013-07-17},
  institution = {Utrecht University},
  location = {Utrecht},
  url = {http://dspace.library.uu.nl/handle/1874/278174},
  urldate = {2016-04-04},
  abstract = {This ethnography examines the construction of a Secondary World within Live Action Role Play. Based on five months of anthropological research in the United Kingdom, its aim is to place LARP within the context of existing anthropology on play and performance and examine how LARP constructs an alternate reality for the players. LARP is separated from the mundane world by the “magic circle”, created by explicit rules and the social contract. In game, players strive for immersion, similar to the concept of flow, in which players feel that they are their characters. This immersion is enabled by character performance, a fully sensorial atmosphere, and personal relationships between the characters. Once immersed, players no longer feel as if they are acting, but rather that the character has taken over. This immersion allowed players to enter the Enchanted State and an alternate reality in which characters are performative and actions within game were referred to as authentic. This adult play allows players to construct a fragile alternate reality based on the belief of its players.},
  langid = {english},
  keywords = {_LARP,_SOCIOLOGY,=cleaned}
}

