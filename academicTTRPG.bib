}

@article{montola_painful_2011,
  title = {The {{Painful Art}} of {{Extreme Role-Playing}}},
  author = {Montola, Markus},
  date = {2011},
  journaltitle = {Journal of Gaming and Virtual Worlds},
  volume = {3},
  number = {3},
  pages = {219--237},
  issn = {1757-1928(ELECTRONIC),1757-191X(PRINT)},
  doi = {10.1386/jgvw.3.3.219_1},
  abstract = {Fun is often seen as a necessary gratification for recreational games. However, like all other forms of art, games can also be created to convey painful experiences. This article studies two non-digital freeform role-playing games, The Journey and Gang Rape, that aim to create extreme experiences of tragedy, horror, disgust, powerlessness and self-loathing. Even though the games successfully produce unpleasant experiences, they gratify their target group of experienced role-players. Almost all of the sixteen interviewed players appreciated their experiences, despite crying, experiencing physiological stress reactions and feeling generally ‘bad’ during play. It was also found out that players feel intimately connected after play, and felt the need for thorough debriefing afterwards. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  issue = {3},
  langid = {english},
  keywords = {_EMOTIONS,_LARP,_peer reviewed,_problematic,_sex-transgressive,_SOCIOLOGY,=0.66,=cleaned}
}

