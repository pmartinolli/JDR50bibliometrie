@article{campbell_nothing_2021,
  title = {Nothing {{Like}} a {{Good Fiasco}}! {{Exploring}} the {{Potential}} of {{Tabletop Role-Playing Games}} ({{TRPGs}}) {{As Literacy Experiences}}},
  author = {Campbell, Harrison and Madsen, Andrea},
  date = {2021-12-24},
  journaltitle = {Canadian Journal for New Scholars in Education/ Revue canadienne des jeunes chercheures et chercheurs en éducation},
