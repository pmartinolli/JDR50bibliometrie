  langid = {english},
  keywords = {_actual play,_computer rpg,_dnd_game,_EMOTIONS,_empirical work,_fandom,_game studies,_GAMEPLAY,_identity,_MEDIA,_nostalgia,_OTHER GAMES,_TTRPG,=cleaned,Computer Science Education,CSEd,Human Use of Human Beings,Norbert Wiener,Tabletop Games,Vintage Computing}
