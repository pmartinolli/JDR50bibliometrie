@thesis{sharp_conversational_2023,
  type = {Master of Science In Speech, Language, and Hearing Sciences},
  title = {Conversational {{Analysis}} of {{Turn-Taking}} in a {{Dungeons}} \& {{Dragons Game Session}}},
  author = {Sharp, A J},
  date = {2023},
  institution = {San Francisco State University},
  location = {San Francisco, CA},
  url = {https://scholarworks.calstate.edu/downloads/r494vs44n},
  abstract = {Tabletop roleplaying games are experiencing a resurgence in popularity and have been used in pragmatics therapy and for social-emotional learning. However, little research exists to describe how players of a game construct and uphold pragmatic norms during gameplay. Using a synthesis of conversation analysis and frame analysis techniques, this study seeks to understand how one group of professional gamers navigates conversational turn-taking during gameplay. 1.5 hours of gameplay conversation was transcribed using Jeffersonian transcription, and twelve selected segments of the transcript were analyzed based on framing, use of character voices, and overlapping talk. Results indicate that turn-taking is weakly affected by use of character voice and performance, as well as affected by strong emotions and the narrative significance of each utterance. Individual tendencies regarding overlapping talk are not uniform across a group but are not necessarily punished for not conforming to an overall norm. This study stands as a unique example of how conversation analysis and frame analysis can be used in conjunction to analyze the conversational norm-building of a social group.},
  langid = {english},
  pagetotal = {84},
  keywords = {_conversation,_dnd_game,_GAMEPLAY,_oral communication,_TTRPG,=cleaned}
}

