@article{caira_theorie_2016,
  title = {Théorie de la fiction et esthétique des jeux},
  author = {Caïra, Olivier},
  date = {2016},
  journaltitle = {Sciences du jeu},
  number = {6},
  issn = {2269-2657},
  doi = {10.4000/sdj.671},
  url = {http://sdj.revues.org/671},
  urldate = {2016-11-11},
  abstract = {Les jeux occupent aujourd’hui une place centrale dans la palette des fictions. Si les jeux abstraits constituent un pôle autonome d’expression logico-mathématique, les jeux de simulation sont en revanche créés par association d’un thème et d’un système, ce qui les rend à la fois mimétiques et axiomatiques. On étudie ici différentes manifestations de plaisir esthétique liées à l’expérience des jeux, indépendamment de leur ornementation visuelle ou sonore. Pour les jeux abstraits, on identifie trois motifs~: le sublime qui reflète notre incapacité à embrasser l’immensité d’une combinatoire, le subtil qui se révèle lors de la résolution d’énigmes originales ou difficiles, et le merveilleux qui découle de la découverte de propriétés inattendues du système. Les jeux de simulation sont étudiés en deux temps~: les jeux de plateau et de rôle dont le système fonctionne comme une épure, et les jeux vidéo qui associent performance générale du moteur et création d’écarts esthétiques par le détail.},
  issue = {6},
  langid = {french},
  keywords = {_boardgame,_computer rpg,_literature,_peer reviewed,_PSYCHOLOGY,_TTRPG,_typology,=0.66,=biblio:DNDP2018,=bibliography analyzed,=blogged,=cleaned,=french,=p0,=PM-lu,=rabbit},
  annotation = {QID: Q114731348}
}

