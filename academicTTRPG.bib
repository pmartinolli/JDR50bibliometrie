@thesis{mills_can_2022,
  type = {Psy.D. Clinical Psychology},
  title = {Can {{I Level Up}} to {{Real Life}}? {{A Study}} of the {{Effects}} of {{Role-Playing Games Combined}} with {{Guided Character Development}} on {{Goal Attainment}} and {{Self-Efficacy}}},
  shorttitle = {Can {{I Level Up}} to {{Real Life}}?},
  author = {Mills, Adam D.},
  date = {2022},
  institution = {Chicago School of Professional Psychology},
  location = {Chicago},
  url = {https://www.proquest.com/docview/2702477419/abstract/7CD2EB67C4004E9CPQ/1},
  urldate = {2022-08-29},
  abstract = {With the prevalence of gaming among adolescents and young adults, it is essential to understand its impact on development. This study aims to facilitate development using techniques derived from Tabletop Role Playing Games (TTRPGs). This study explored the effects of role-playing activities, combined with guided character development on goal attainment. The study examined several factors of goal attainment, including motivation and self-efficacy. The study occurred over six bi-weekly one-hour-long sessions. The researcher met with the participants individually, using semi-structured open-ended question interviews and guided character-development techniques. Between each session, the participants role-played their characters. They kept a daily journal of goal-oriented activities, thoughts, and goals, which were reviewed during the bi-weekly interview and used for developing or leveling up each participant’s character. The researcher hypothesized that improvement in the character’s performance would positively impact the goal attainment and self-efficacy of the role-playing individual. The sample included five participants ranging in age from 27 to 28 years old and were recruited via flyers posted at local game stores, online gaming groups and distributed by the researcher. The results showed that utilizing areas of interest such as gaming can increase self-efficacy and goal attainment while engaging in goal-oriented activities.},
  isbn = {9798841759089},
  langid = {english},
  pagetotal = {93},
  keywords = {_LEARNING,_motivation,_PSYCHOLOGY,_quest,_self-efficacy,_soft skills,_TTRPG,=cleaned,Motivation}
}

