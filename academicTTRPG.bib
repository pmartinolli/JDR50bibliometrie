@inproceedings{pothong_deliberating_2021,
  title = {Deliberating {{Data-Driven Societies Through Live Action Role Play}}},
  booktitle = {Designing {{Interactive Systems Conference}} 2021},
  author = {Pothong, Kruakae and Speed, Chris and Catlow, Ruth and Dixon, Billy and Morgan, Evan and Knowles, Bran and Newmarch, Georgia and Richards, Daniel and Cruickshank, Leon},
  date = {2021-06-28},
  series = {{{DIS}} '21},
  pages = {1726--1738},
