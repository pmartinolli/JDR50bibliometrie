@incollection{agnew_reenacting_2022,
  title = {Reenacting {{Japan}}'s {{Past That Never Was}}. {{The Ninja}} in {{Tourism}} and {{Larp}}},
  booktitle = {Reenactment {{Case Studies}}: {{Global Perspectives}} on {{Experiential History}}},
  author = {Kamm, Björn-Ole},
