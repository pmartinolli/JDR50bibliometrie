  volume = {14},
  eprint = {38250272},
  eprinttype = {pmid},
  pages = {1276757},
  issn = {1664-0640},
  doi = {10.3389/fpsyt.2023.1276757},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC10796767/},
  urldate = {2024-01-30},
  abstract = {Background The area of social skills is broad, in theory and in practice. For social skills training, various clinical practices have been applied in group sessions, as have motivational resources such as role-playing games (RPGs). In recent years, the need arose to assess the clinical impact of the pandemic. The objective of this study was to determine the impact that the pandemic has had on in-person and online social skills training. Methods We evaluated six subjects with autism spectrum disorder, with or without another, similar disorder, each of whom attended a total of 12 two-hour RPG sessions over a 12-month period. The original (Portuguese-language) version of the Social Skills Inventory for Adolescents was applied at three different time points (pre-, mid-, and post-intervention). Results After six in-person tabletop RPG sessions, there was an increase in the mean frequency scores and a decrease in the mean difficulty scores. However, during the pandemic, the remaining six sessions were conducted online and the effect was the opposite. Conclusion Our data indicate that there is a need for further studies assessing social skills training in online contexts.},
  langid = {english},
  pmcid = {PMC10796767},
  keywords = {_covid19,_LEARNING,_peer reviewed,_soft skills,_therapy,_TTRPG,_virtual table,=cleaned,behavior analysis,Group psychotherapy,Online therapy,Pandemics,Social skills training}
}

