@article{thoren_pen_2020,
  title = {Pen, Paper, Dice…screen? {{Digital}} Resistance in the {{Swedish}} Tabletop Role-Playing Game Community},
  shorttitle = {Pen, Paper, Dice…screen?},
  author = {Thorén, Claes},
  date = {2020},
  journaltitle = {Convergence: The International Journal of Research into New Media Technologies},
  shortjournal = {Convergence},
