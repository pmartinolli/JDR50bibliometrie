@thesis{glickman_roll_2022,
  type = {PhD Psychology},
  title = {Roll for {{Wellness}}: {{A Therapeutic Role-Playing Treatment}} for {{LGBTQIA}}+ {{Identified Individuals}} in {{Group Therapy Settings}}},
  shorttitle = {Roll for {{Wellness}}},
  author = {Glickman, Jacob},
  date = {2022},
  institution = {Widener University},
  location = {Chester},
  url = {https://www.proquest.com/docview/2659667437/abstract/720400AA9CD648EBPQ/1},
  urldate = {2022-05-12},
  abstract = {The LGBTQIA+ community is a complex, expansive, and resilient population with a focus on identity formation, which often begins in childhood. Due to external threats and resultant internalized oppression, however, individuals can experience identity distress in addition to a host of other mental health challenges, exacerbated by the difficulty of finding culturally-informed mental healthcare. Gay affirmative therapy and forms of narrative therapy developed in response to this need; additionally, the camaraderie of group settings holds special significance for the LGBTQIA+ community, and existing research highlights the efficacy of group treatment for queer-identified individuals. As initial, promising data suggest tabletop role-playing games (TTRPGs) can promote self-insight and enhance connection within a therapeutic context, further examination shows that TTRPGs also utilize many of the widely-accepted principles of group therapies like psychodrama, play therapy, and adventure therapy, thus challenging decades of erroneous linkage between such games and the development of psychopathology. Due to their inherent focus on community-building and affirmative identity exploration through the enhanced safety of narrative transference, TTRPGs offer a promising framework for implementing best practice with LGBTQIA-oriented treatment groups. This dissertation introduces an updated structure for TTRPGs called Roll for Wellness that highlights therapeutic elements with a rulebook created specifically for use in group therapy settings, reimagined character creation template, and an initial campaign designed to focus on queer identity development with optional content about depression.},
  isbn = {9798426851542},
  langid = {english},
  pagetotal = {153},
  keywords = {_anxiety,_lgbtq,_PSYCHOLOGY,_SOCIOLOGY,_therapy,_TTRPG,_well-being,=cleaned}
}

