@inproceedings{koukia_online_2014,
  title = {Online {{Versus Pen And Paper Role Playing Games}}. {{A Case Of}} "{{Internet Addiction}}"?},
  booktitle = {Pluralism in {{Psychiatry}}: {{I}}. {{Diverse Approaches}} and {{Converging Goals}}},
  author = {Koukia, E. and Mangoulia, P.},
  editor = {Soldatos, C. and Ruiz, P. and Dikeos, D. and Riba, M.},
  date = {2014},
  pages = {59--64},
  publisher = {Medimond International Proceedings},
  location = {Athens},
  abstract = {Massive Multi-Player Online Role-Playing Games (MMORPGs) and Multi-User Domain Games (MUDs) are the most popular genre of PC games. In recent years there is a growing interest whether this type of Internet use, represents an "addiction". We recruited 22 former players (14 players of Internet and 8 players of pen and paper Role Playing Games - RPGs; mean age 27 +/- 5years who had succeeded to cut back the game without external help. We tried to describe players' perceptions of their previous "addiction" and compare their views. The study data was collected through personal interviews. Semi-structured interviews were tape-recorded, transcribed and subjected to qualitative content analysis. Based on the results, the players' expressed various perceptions, which constituted five categories: games' particular characteristics, negative effects and positive effects on players' lives, possible causes for their behavior, and future intentions. Significant differences were noted in players' perceptions between the two groups. For Internet users content analysis revealed the same basic problems: irritability, social life neglect, and disruption of relationships, sleep deprivation, school or university failure, and loss of interest in other activities. Younger players reported even more problems. Overall, Internet players attributed their problems to an "addictive dynamic". None of these problems existed in players of pen and paper RPGs. Those players reported feelings of satisfaction, pleasure, improved social interactions and friendship connected to the game. Four players who were adolescents, all former Internet players, characterized their previous Internet use as an "addiction". They were seeing their problems as more serious and disruptive for their lives. In conclusion, the majority of internet players recognize the "addictive dynamic" of the game, but they do not necessarily consider themselves as "addicted". They characterize their Internet use and its consequences as a "choice"; time loss and difficulty breaking off from the game were the more serious problems faced mainly by Internet players. It should be noted, however, that in contrast to clearly "addicted players", the players of this study were all able to overcome their psychological and behavioral problems without any external help after successful cessation of the Internet use.},
  eventtitle = {{{WPA Thematic Conference}} on {{Intersectional Collaboration}}},
  isbn = {978-88-7587-698-2},
  langid = {english},
  keywords = {_belonging,_computer rpg,_HISTORYOFRPG,_moral panic,_TTRPG,=cleaned},
  annotation = {WOS:000343669700013}
}

