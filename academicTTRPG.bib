@book{schmidt_zwischen_2012,
  title = {Zwischen Simulation und Narration: Theorie des Fantasy-Rollenspiels- Mit einer Analyse der Spielsysteme Das Schwarze Auge, Shadowrun und H.P. Lovecraft's Cthulhu},
  shorttitle = {Zwischen Simulation und Narration},
