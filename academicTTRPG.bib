  keywords = {_fandom,_gender,_minority,_peer reviewed,_SOCIOLOGY,_TTRPG,_wod_game,=cleaned,exalted,Exalted,fantasy,feminist critical discourse analysis,geek culture,gender and sexuality,gender representation,popular culture,race,role-playing game (RPG),roleplaying games}
}

