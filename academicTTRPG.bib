  title = {Social Skills Training with a Tabletop Role-Playing Game, before and during the Pandemic of 2020: In-Person and Online Group Sessions},
  shorttitle = {Social Skills Training with a Tabletop Role-Playing Game, before and during the Pandemic of 2020},
  author = {Henning, Germano and family=Oliveira, given=Reinaldo Rodrigo, prefix=de, useprefix=true and family=Andrade, given=Marcus Túlio Pereira, prefix=de, useprefix=true and Gallo, Renato Villela and Benevides, Raissa Roberti and Gomes, Rodrigo Antonio Fuga and Fukue, Lucas Eiji Kong and Lima, Arthur Vaciloto and family=Oliveira, given=Maria Beatriz Baggio Z. N., prefix=de, useprefix=true and family=Oliveira, given=Daniel Amorim Medeiros, prefix=de, useprefix=true and Werpp, Morgana and Moraes, Lucas and Neto, Francisco Lotufo},
  date = {2024-01-05},
