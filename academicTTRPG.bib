  url = {http://www.jstor.org/stable/3341129},
  urldate = {2015-05-01},
  abstract = {This paper examines the problem of Satanism using a social constructionist framework. Seventy-five Canadian news articles from 1980-1989 were examined and telephone interviews carried out to obtain a picture of how the problem is emerging, the groups involved, and the claims being made. An examination of the use of rhetoric in claims is undertaken. The role of the news media and American experts in promoting the problem is then explored. The theme of Satanism as a vehicle for other social problems, how claims about it constitute a symbolic crusade, and Satanism's natural history as a social problem in Canada are discussed. /// Cet article examine la satanisme au moyen d'une approche constructionniste. Soixantequinze articles d'information canadiens ont été analysés et des entrevues téléphoniques ont été réalisées, afin de comprendre l'émergence du problème et d'identifier les groupes et les énoncés impliqués. La réthorique de ces énoncés est examinée, ainsi que le rôle des média et des experts américains dans la promotion de ce problème. Le thème du satanisme comme véhicule d'autres problèmes sociaux, la façon dont les énoncés à son sujet constituent une croisade symbolique, et l'histoire naturelle du satanisme en tant que problème social au Canada sont aussi explorés.},
  langid = {english},
  keywords = {_HISTORYOFRPG,_moral panic,_peer reviewed,_TTRPG,=cleaned},
  annotation = {QID: Q114772094}
}

@article{lis_psychiatrists_2015,
  title = {Psychiatrists’ {{Perceptions}} of {{Role-Playing Games}}},
