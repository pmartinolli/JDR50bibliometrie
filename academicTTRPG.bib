  location = {New York, NY, USA},
  doi = {10.1145/3649921.3650018},
  url = {https://doi.org/10.1145/3649921.3650018},
  urldate = {2024-07-08},
  abstract = {In 2023, three major games introduced they/them pronouns into their character creation options: Baldur's Gate 3 (Larian Studios), Starfield (Bethesda); and Story of Seasons: A Wonderful Life (Marvelous). Each of these games comes from companies or series with a history of enforcing binary gender. Making such shifts to include both gender equity and same-sex romance sparked backlash, particularly at a moment when “woke” cultural production is under intense cultural scrutiny. By examining the responses to these three very different, but culturally significant, games, we contextualize the importance of meaningful gender and sexuality inclusivity in gaming given a history of reactionary movements in gaming culture (most notoriously Gamergate). Critically, all of these games became bestsellers, suggesting that while “go woke, go broke” might remain a rallying cry in small portions of gaming subculture, the move towards inclusivity is ultimately not harming the commercial success of these games.},
  isbn = {9798400709555},
  langid = {english},
  keywords = {_gender,_SOCIOLOGY,_TTRPG,=cleaned}
}

