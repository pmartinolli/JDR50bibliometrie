@thesis{dauphragne_du_2010,
  type = {Thèse pour le doctorat en sciences de l’éducation},
  title = {Du savoir historique au savoir ludique : La médiatisation de l'histoire dans les jeux de rôles},
  shorttitle = {Du savoir historique au savoir ludique},
  author = {Dauphragne, Antoine},
  date = {2010-12},
  institution = {Université Paris 13},
  location = {Paris},
  url = {https://hal.archives-ouvertes.fr/tel-02079905},
  urldate = {2022-02-16},
  abstract = {Abstract : This research focuses on how a specific game support, the roleplaying game, captures the historical material. Through the analysis of a game corpus, we aim at understanding the mediatisation process of historical knowledge (the translation of history into a medium) in a game-related perspective. Firstly, we consider the historiographical dimension of history. Historians’ reflections on their discipline, but also works on history as a genre, clearly show that the relation between historical knowledge and fiction/narrative is ambiguous. Secondly, we focus on the theoretical tools that may help us to find a connection between history’s seriousness and game’s triviality. As the approach and use of historical knowledge by a game-related medium are not involved with formal and pedagogic learning processes, we confront them to other theoretical tools such as studies about informal learning and the notions of knowledge circulation. Thirdly, we concentrate on the presentation of our research object: roleplaying game. Roleplaying game, which is an ambivalent field as well as a medium whose distribution is limited within networks of the mass gaming culture, has distinctive features such as the completeness and coherence of its imaginary worlds. The last part of our work aims at analysing these features and their relation to the mediatisation of historical knowledge by roleplaying games. The construction of fictional worlds “recycling” history and the setting up of game mechanisms reflecting play uses of historical resource appear as the two major elements driving the transformation process of historical knowledge into ludic knowledge. Finally, history mediatisation within roleplaying games should not be confronted to criteria of historical validity but should be considered as an appropriation process of knowledge via game-related fiction.},
  langid = {french},
  keywords = {_france,_history,_LEARNING,_TTRPG,=cleaned,corpus,culture ludique,éducation informelle,game study,gaming culture,histoire,historical knowledge,history,informal learning,jeu de rôles,knowledge circulation,mediatisation,mobilité des savoirs,roleplaying games,savoirs historiques,sciences du jeu,simulation}
}

