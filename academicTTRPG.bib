@incollection{bowman_magic_2021,
  title = {Magic Is {{Real}}: {{How Role-playing Can Transform Our Identities}}, {{Our Communities}}, and {{Our Lives}}},
  shorttitle = {Magic Is {{Real}}},
  booktitle = {Book of {{Magic}}: {{Vibrant Fragments}} of {{Larp Practices}}},
  author = {Bowman, Sarah Lynne},
  editor = {Djukastein, Kari and Irgens, Marcus and Lipsyc, Nadja and Sunde, Lars Kristian Løveng},
  namea = {Hugaas, Kjell Hedgard},
  nameatype = {collaborator},
  date = {2021},
  publisher = {Knutepunkt},
  location = {Oslo},
  url = {http://urn.kb.se/resolve?urn=urn:nbn:se:uu:diva-462545},
  urldate = {2022-01-02},
  abstract = {This article includes an in-depth discussion of the “magical” potential of role-playing. We describe some of the barriers to transformation that can arise from alibi, cognitive dissonance, role-distancing, and the pressures of conformity. We will then examine role-playing from two quite different lenses: a) Conceptualizations of ritual, aspecting, and manifestation in occult and metaphysical traditions; and b Research in the social sciences about the power of thought and narrative upon self-concept, behavior, performance, and well-being. This preliminary exploration of concepts that might help explain the potential of role-playing as a form of postmodern “magic” is by no means exhaustive or detailed. Rather, we present vignettes of thought from various areas of spiritual practice and social science. We explore how role-playing, perspective taking, narrative, ritual, and the conscious use of specific imaginative practices can directly impact people’s performance at tasks, their self-concepts, and their perceived agency. Then, we examine different models of bleed theory, investigating ways that we can raise awareness around bleed effects and consciously steer toward or away from them as needed. If we intentionally emphasize responsibility, safety, and growth in our communities, we can imagine the role-playing space as a transformational container within which we can explore our edges and mold our self-concepts through play. We can use alibi as a tool to permit greater experimentation, while decreasing its strength when we wish to transfer skills, insights, and personality traits outside of the magic circle. Finally, through conscious and deliberate integration practices, we can distill these insights and more permanently infuse our lives with this magic, manifesting new conceptions of self, of community, of relationships, and of our life potential.},
  langid = {english},
  keywords = {_bleed,_distantiation,_embodiment,_EMOTIONS,_fandom,_identity,_LARP,_PSYCHOLOGY,_ritual,_social contract,_SOCIOLOGY,_transformative,_TTRPG,_well-being,=cleaned}
}

