  url = {https://www.proquest.com/docview/3083766485/abstract/F71FE5F2A5A942EDPQ/1},
  urldate = {2024-07-29},
  abstract = {The purpose of this qualitative study was to explore the experiences of live action role play (LARP) participants, investigate creativity’s relevance to this experience, and determine how LARP supports creativity. A number of studies have provided evidence that playful individuals are more creative, and that other forms of role play such as improv role play and role play games (RPGs) increase psychometric measures of creativity; however, there is a gap in the research regarding qualitative investigations into the creative play associated with LARP. Semi-structured focus group interviews were conducted with two focus groups, yielding narrative data delivered by a total of nine participants. Content analysis revealed four themes that were significant to the experience of LARP players: community, creativity, play, and transformation. The data gathered regarding creativity substantiates previous studies about play and creativity, but this study offers significantly more information than is available on player experience and other topics and fills some of the current research gap in the intersectional field of adult play, creativity, and LARP.},
  isbn = {9798383563632},
  langid = {english},
  pagetotal = {91},
  keywords = {_creativity,_fandom,_LARP,_LEARNING,_SOCIOLOGY,_transformative,=cleaned,Adult play,Art education,Community,Creativity,Live action role play,Role play games,Sociology,Transformation}
}

