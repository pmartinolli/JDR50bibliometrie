@thesis{mendoza_gaming_2020,
  type = {Master Expressive Therapies},
  title = {Gaming {{Intentionally}}: {{A Literature Review}} of the {{Viability}} of {{Role-Playing Games}} as {{Drama-Therapy-Informed Interventions}}},
