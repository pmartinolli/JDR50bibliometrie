@incollection{peterson_10_2024,
  title = {10 {{The Return}} of the {{Referee}}},
  booktitle = {Playing at the {{World}}, {{2E}}: {{The Invention}} of {{Dungeons}} \& {{Dragons}}},
  author = {Peterson, Jon},
  date = {2024},
