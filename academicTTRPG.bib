@article{kankainen_curating_2024,
  title = {Curating the {{Self-Identity}}: {{Tabletop Game Hobbyist Online Practices}} as an {{Extended Leisure Experience}}},
  shorttitle = {Curating the {{Self-Identity}}},
  author = {Kankainen, Ville},
  date = {2024},
