  author = {Bean, Anthony},
  date = {2023-11-30},
  journaltitle = {Journal of Psychology and Psychotherapy Research},
  shortjournal = {J. Psychol. Psychother.Res},
  volume = {10},
  pages = {97--105},
  issn = {2313-1047},
  doi = {10.12974/2313-1047.2023.10.09},
  url = {https://savvysciencepublisher.com/index.php/jppr/article/view/958},
  urldate = {2024-10-15},
  abstract = {This paper presents a revolutionary theory in psychology, delving into the transformative possibilities of utilizing geek culture as a potent catalyst for behavioral change. In contemporary society, geek culture, defined by fervent interests in video games, comics, science fiction, and technology, has emerged as a prominent and influential force. The theory advanced here contends that the captivating and immersive qualities inherent in geek culture can serve as a compelling tool to shape and redirect behaviors. By leveraging the passion and engagement fostered within geek communities, this theory proposes a distinctive avenue for psychological intervention. It explores the prospect of channeling the intrinsic motivation and communal dynamics prevalent in geek culture to enact meaningful and positive changes in individuals' behaviors. This innovative approach signifies a departure from conventional psychological interventions, positioning geek culture as an untapped resource with the potential to redefine the landscape of behavioral change strategies.},
  langid = {english},
  keywords = {_fandom,_OTHER GAMES,_peer reviewed,_PSYCHOLOGY,_SOCIOLOGY,_theoretical work,_therapy,_transformative,_TTRPG,_well-being,=cleaned}
}

