@thesis{dekraker_lang-ree_therapeutic_2023,
  title = {The {{Therapeutic Power}} of {{Theater}}: {{Combating}} the {{Rise}} in {{Childhood Anxiety}} and {{Empowering Students}} with {{Dramatic Storytelling}}},
  shorttitle = {The {{Therapeutic Power}} of {{Theater}}},
  author = {DeKraker Lang-Ree, Madeline},
  date = {2023},
  institution = {UC Santa Cruz},
  url = {https://escholarship.org/uc/item/6292m9kg},
  urldate = {2023-08-10},
  abstract = {As factors such as a global pandemic, school shootings, and climate change continue to affect the rapid rise in childhood anxiety, there is an ever-pressing need for the therapeutic power of dramatic storytelling in our classrooms. Though art has been recognized as therapeutic both in the psychological and cultural realms for generations, it is now essential to infuse dramatic storytelling practices into the pedagogies and curricula of our grade school classrooms.The isolation created by pandemic shutdowns \&amp; social distancing, sociocultural \&amp; political warfare in the U.S., and social media addictions, leaves us with less and less time to connect on a basic human emotional level. Dramatic storytelling provides an opportunity to build community, self-reflect, and find growth and healing.During an eight-week period, I executed a dramatic storytelling workshop through the ArtsBridge America fellowship program at UC Santa Cruz, which connected me with a local, underfunded public school. Through documentation of the lesson plans, reflections on the classes, and analysis of the students' experiences, I argue for implementing dramatic storytelling as a necessary and invaluable power.},
  langid = {english},
  keywords = {_PSYCHOLOGY,_theater,_therapy,_TTRPG,=cleaned}
}

