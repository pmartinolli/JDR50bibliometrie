@incollection{fernandez_pichel_egypt_2023,
  title = {Egypt and {{Role-Playing Games}}. {{Does}} the {{World}} of {{Darkness Universe Use Ancient Egyptian Sources}}?},
  booktitle = {How {{Pharaohs Became Media Stars}}: {{Ancient Egypt}} and {{Popular Culture}}},
  author = {Fernández Pichel, Abraham I. and Sanchez Dominguez, Victor},
