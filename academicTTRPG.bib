@thesis{caira_les_2007,
  type = {Thèse de doctorat en Sociologie},
  title = {Les contre-allées de l'expérience : vers une sociologie comparative de l'engagement fictionnel},
  author = {Caïra, Olivier},
  date = {2007},
  institution = {EHESS},
  location = {Paris},
  url = {https://www.sudoc.fr/178761990},
  abstract = {Cette thèse propose une définition unifiée de la fiction et une grille d'analyse des formes d'engagement qu'elle suscite, afin de favoriser la comparaison d'expériences variées : littérature, cinéma, théâtre, jeux de rôle, jeux vidéo, exercices, fictions philosophiques ou mathématiques, etc. Inspirée des traditions ontologique, formelle, pragmatique et cognitive, la définition proposée ici exclut toute référence à la représentation, à la narration, au langage et à la logique. Elle repose sur la levée des contraintes de preuve : par recoupement pour les fictions mimétiques (roman, film. . . ), par isomorphisme pour les univers axiomatiques (jeu d'échecs, Tetris. . . ). Entre ces deux pôles apparaît une famille méconnue : les jeux de simulation, qui lient la visée mimétique (le monde à simuler) à un dispositif logicomathématique (le simulateur).},
  langid = {french},
  pagetotal = {562},
  keywords = {_SOCIOLOGY,_TTRPG,=cleaned,=french},
  annotation = {Sous la direction de Francis Chateauraynaud.}
}

@inproceedings{caira_mobilization_2014,
  title = {Mobilization of {{Knowledge}} and {{Narrative Improvisation}} in {{Storygames}}},
  booktitle = {Narrative {{Matters}} 2014: {{Narrative Knowing}}/{{Récit}} et {{Savoir}}},
  author = {Caïra, Olivier},
  date = {2014},
  pages = {1--6},
  location = {Paris},
  url = {https://hal.archives-ouvertes.fr/hal-01099219/},
  urldate = {2015-09-16},
  abstract = {Storygames are communication devices specifically designed to spark collective improvisation in natural language, using different parts of a fictional diegesis : characters, objects, actions, places, relationships, issues... Often overshadowed by video games or confused with tabletop role-playing games, these recently developed games remain overlooked by most scholars, although they provide ideal observation conditions and new research avenues for narratologists and interactivity theorists. Since the publication of Once Upon a Time (1994), dozens of games have enriched the diversity of available narrative traditions in this field: fairy tales, horror B-movies (Polar Base), historical drama (Montségur 1244) or the Coen brothers’ universe (Fiasco). As a product, a storygame does not display any narrative, but provides rules and tools (dice, cards, blank forms) that can generate a continuously renewed narrative production. This is why the issue of how knowledge is mobilized during storygame sessions is crucial to the ethnographic study of the links between narration and interactivity. Collective encyclopedias – in Umberto Eco’s sense – allow the sharing and the development of a plot, on the basis of sparse diegetic information, in “rotating narrator” devices where interpretative cooperation failures often lead to in-game conflict.},
  keywords = {_fiasco_game,_GAMEPLAY,_improvisation,_OTHER GAMES,_story vs game,_storygame,_storytelling,_TTRPG,=cleaned}
}

