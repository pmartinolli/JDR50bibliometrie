@thesis{kraus_social_1986,
  type = {PhD Sociology},
  title = {Social {{Psychological Causes}} and {{Consequences}} of {{Fantasy Gaming}}: {{A Test}} of {{Identity Theory}}},
  shorttitle = {Social {{Psychological Causes}} and {{Consequences}} of {{Fantasy Gaming}}},
  author = {Kraus, Linda Ann},
  date = {1986},
  institution = {Indiana University},
  location = {Bloomington, IN},
  url = {http://search.proquest.com/pqdtglobal/docview/303487060/abstract/6D46F700A1F54C01PQ/17?accountid=12543},
  abstract = {Identity theory argues that the self (i.e., viewing oneself from the standpoint of others) is composed of multiple identities, or internalized positional designations. Salience, or the probability of an identity being invoked in a given situation, is argued to have two causes, interactional commitment (embeddedness in an identity-related network) and affective commitment (emotional gratification derived from an identity), and to in turn result in identity-related behavior. This dissertation addresses three limitations of past research: the neglect of leisure identities, the operationalization of behavior as time spent in role rather than as specific types of behavior, and the failure to include all four identity theory concepts in a single study. Data were collected in July 1983 for 113 boys (aged 10 to 17) attending a camp devoted to Dungeons and Dragons gaming. Campers completed surveys administered each of the five days of camp. Factor analyses established scales of each concept, while path analyses determined the relationships among the concepts. The results indicate the importance of delineating among types of behavior; interactional commitment consistently affects social behaviors, while affective commitment consistently affects solitary behaviors. Both types of commitment affect materialistic behavior. Dividing the sample on such variables as age and the length of time one had been gaming indicates that involvement with social behavior occurs the longer one has been gaming; this can be seen as support for one's ability and inclination to role-make, the search for self-validation, and development within identities. Contrary to the theory, interactional commitment is seldom related to salience, and salience rarely affects behavior. Identity theorists studying adolescents are urged to consider the possibility that the salience hierarchy of teens and pre-teens may be so unstable or ill-formed that weak or anomalous findings with regard to salience may result.},
  langid = {english},
  pagetotal = {95},
  keywords = {_dnd_game,_identity,_PSYCHOLOGY,_SHS,_TTRPG,=cleaned}
}

