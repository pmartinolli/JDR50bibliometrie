@incollection{brom_towards_2015,
  title = {Towards in {{Situ Measurement}} of {{Affective Variables During Playing Educational LARPs}}: {{A Pilot Study}}},
  shorttitle = {Towards in {{Situ Measurement}} of {{Affective Variables During Playing Educational LARPs}}},
  booktitle = {Proceedings of the 9th {{European Conference}} on {{Games Based Learning}} (Ecgbl 2015)},
  author = {Brom, Cyril and Dobrovolny, Viktor and Bromova, Edita},
