@thesis{gutierrez_therapy_2017,
  type = {MA Social Work},
  title = {Therapy \& {{Dragons}}: {{A}} Look into the {{Possible Applications}} of {{Table Top Role Playing Games}} in {{Therapy}} with {{Adolescents}}},
  shorttitle = {Therapy \& {{Dragons}}},
  author = {Gutierrez, Raul},
  date = {2017},
  institution = {California State  University, San Bernardino},
  location = {San Bernardino},
  abstract = {The purpose of this study was to gain a better understanding on the use of Table Top Role Playing Games (TRPGs) in therapy with adolescents. This qualitative study utilized individual interviews as a means to extract themes from mental health practitioners who have provided this intervention with their clients in an individual/group therapy setting. Results were transcribed to written form. Qualitative analysis procedures were followed to identify themes and subthemes. Areas of particular interest to the social work profession were: applicability to clients of any age, the client-tailored narrative, use of TRPGs in a group setting, TRPGs as a form of exposure therapy. Possible risks of utilizing this intervention and limitations of the study were also introduced.},
