@thesis{smith_correlation_2013,
  type = {Ph.D. Psychology},
  title = {The {{Correlation Between General}} and {{Social Self-Efficacy}} and {{Participation}} in {{Fantasy Roleplaying Games}}},
  author = {Smith, Gregory Brooks},
  date = {2013},
  institution = {Walden University},
  location = {Minneapolis, MN},
  url = {http://search.proquest.com/pqdtglobal/docview/1459215822/abstract/30959A5791F243C7PQ/1},
  urldate = {2016-09-02},
  abstract = {Self-efficacy (SE), the theoretical basis for the study, is the internal belief that one is capable of appropriately addressing a given situation. High SE is linked to positive outcomes in diverse areas such as academics, health, and career. Therapeutic role play has been found to increase SE through mastery experiences, yet the correlation between roleplaying games (RPGs) and general and social SE has remained unexamined. In this study, the SE of gamers was examined for a statistically significant correlation between roleplaying activities and SE over that of the general, nonroleplaying population. A convenience sample of 414 adult gamers completed the General Self-efficacy Exam (GSE), Perceived Social Self-Efficacy scale (PSSE), and surveys of RPG activity and demographics. Findings showed a correlation between participation in RPGs and general SE [t(413) = 24.90, p {$<$}. 001], though not between RPGs and social SE. There was no overall correlation between time and general SE, though time was positively correlated with social SE (r = .083, p {$<$} .05). An ANOVA found that GMs have higher levels of both general [F(2, 411) = 30.92, p {$<$} .001] and social [F(2, 411) = 8.978, p {$<$} .001] SE than among individuals who were primarily players or who were equally players and GMs. The multidimensional importance of high SE throughout the life experience makes it vital to identify many avenues for improving SE. Knowledge of the correlation between RPGs and SE is an important contribution to existing literature, opening up the possibility of establishing a causal link between RPGs and SE through experimentation. Findings could enhance social change initiatives through identification of an inexpensive, easily-accessible, and enjoyable method for increasing SE across the lifespan.},
  langid = {english},
  pagetotal = {124},
  keywords = {_LEARNING,_PSYCHOLOGY,_self-efficacy,_therapy,_TTRPG,=cleaned}
}

