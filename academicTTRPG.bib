@thesis{demma_dungeons_2024,
  type = {B.A. in Sociocultural Anthropology,},
  title = {Dungeons \& {{Dragons}}, an {{Analysis}}: {{The Structure}} of {{Gameplay}} and the {{Power}} of {{World Creation}}},
  shorttitle = {Dungeons \&amp; {{Dragons}}, an {{Analysis}}},
  author = {Demma, Andy},
  date = {2024},
  institution = {The George Washington University},
  url = {https://www.proquest.com/openview/a4266deba72bc510e7a32e2d322cad85/1?cbl=18750&diss=y&pq-origsite=gscholar&parentSessionId=uAiD5VxSFLAFmO4VZK1LeVHW%2F%2BZLLeyoCzAuI5n6tr8%3D},
  urldate = {2024-07-29},
  abstract = {Dungeons \& Dragons, known colloquially as D\&D, is a fantasy role-playing game that builds a space for collaborative story-making and gameplay. It also has immense potential to contemplate game structure and play elements as considered within various discussions regarding videogames and role-play. Simultaneously it has the potential to bridge these considerations with the discussions of speculative fiction that have emerged contemplating the impact of imaginative worlds. As an increasingly popular element of our culture, the lack of analysis of D\&D and other role-playing games, especially in discussions of speculative fiction, should be rectified. As individuals are disappearing into vibrant fantasy worlds like the ones built through D\&D, more analysis of them is important. D\&D as a collaborative fantasy world requires immense social interaction and the creation of imaginative elements structured through game rules. It has the potential to provide a lot of anthropological rich data on gameplay, identity creation and maintenance, and collaborative world-building. This paper seeks to tease through the elements of gameplay and world-building to understand the cultural richness built through D\&D campaigns and structurally examine how D\&D is played. Furthermore, I seek to understand how the culture building taking place in this game space has the potential to be deeply impactful in how individuals view and understand our world.},
  langid = {english},
  keywords = {_collaborative,_dnd_game,_GAMEPLAY,_TTRPG,_worldbuilding,=cleaned}
}

