@thesis{soong_when_2023,
  type = {Thesis},
  title = {“{{When You Play DnD}}, {{You Get}} to {{Be More Yourself Than You Are}} in {{Real Life}}”: {{An Investigation}} into the {{RPG Player}}’s {{Experience}} of {{Performance}}},
  shorttitle = {“{{When You Play Dnd}}, {{You Get}} to {{Be More Yourself Than You Are}} in {{Real Life}}”},
  author = {Soong, Joshua Gareth Seow Hui},
  date = {2023-11-14},
  institution = {National University of Singapore},
  location = {Singapore},
  url = {https://scholarbank.nus.edu.sg/handle/10635/246703},
  urldate = {2024-01-23},
  abstract = {The hobby of Tabletop Role-Playing Games has grown significantly in the last five years, with the most well-known game of the genre, Dungeons \& Dragons (DnD), receiving widespread attention. As a game that integrates highly performative elements into its gameplay, DnD is uniquely able to harness play as a means of facilitating players to engage with performance. This thesis is focused on investigating this phenomenon and theorizing on ways to harness it. Through interviews with six TTRPG players, four main aspects of play in DnD were found: Role-play, Social Interaction, Freedom of Expression, and Storytelling. These are the four main “fun factors” of DnD. Each of these aspects were cross-examined through a performance studies lens as well other disciplinary tools such as the MDA framework, a system for analyzing game design by identifying Mechanics, Dynamics, and Aesthetics present in games. Through this process, an experimental game session of DnD was created and play tested by interviewees. Based on findings from interviews and the experimental game session, this thesis concludes that DnD’s primary strength is the creation of a “game-space” for players, a form of protected space in which players are free from external pressures or fears. This space is created by the four fun factors, supported by the framework of a systemized set of rules, and creates a suspension of players’ usual perception of performance, allowing players to engage in performance activities without as much apprehension.},
  langid = {english},
  keywords = {_dnd_game,_identity,_LEARNING,_PSYCHOLOGY,_soft skills,_TTRPG,=cleaned},
  annotation = {Accepted: 2024-01-15T05:14:53Z}
}

