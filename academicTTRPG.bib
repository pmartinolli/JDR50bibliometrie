@article{wyld_collaborative_2020,
  title = {Collaborative Storytelling and Canon Fluidity in {{The Adventure Zonepodcast}}},
  shorttitle = {Collaborative Storytelling and Canon Fluidity in {{The Adventure Zone}} Podcast},
  author = {Wyld, Jasper},
  date = {2020-09-03},
  journaltitle = {Convergence: The International Journal of Research into New Media Technologies},
  shortjournal = {Convergence},
  pages = {1354856520950555},
  publisher = {Sage Publications Inc},
  location = {Thousand Oaks},
  issn = {1354-8565},
  doi = {10.1177/1354856520950555},
  url = {https://journals.sagepub.com/doi/10.1177/1354856520950555},
  abstract = {Podcast fiction storytelling is an underdeveloped area of new media studies. There is a wealth of texts available suitable for exploration. The example ofThe Adventure Zonein particular presents a strong argument that the medium possesses its own singular strengths for storytelling.The Adventure Zoneis a fictional, audio-only, serialised podcast in which a narrative of both considerable length and depth is constructed through the playing of tabletop roleplaying game Dungeons \& Dragons. It is hosted by the McElroy brothers, Griffin, Justin, and Travis, along with their father, Clint.The Adventure Zonedemonstrates the unique qualities and noteworthy potential the podcast medium possesses. The McElroy family collaborates utilising comedy improv practices, which are strengthened by the game's mechanics and rules. The line between characters and players - along with the line between textual and metatextual, canonical and non-canonical, diegetic and non-diegetic data - is significantly blurred through instances such as self-reflexivity and popular culture references. The fourth wall is inapplicable toThe Adventure Zone. It is necessary to re-imagine it instead as a permeable curtain separating the players from the characters. The listeners are provided with a clear view of not only the story ofThe Adventure Zone, but the construction of its creation. There are few mediums in which the audience can so effectively and candidly witness the storytelling process. Comparisons drawn from the original text -The Adventure Zonepodcast - to its ongoing adaptations -The Adventure Zonegraphic novels - illustrates this fact further. The collaborative, improvisational, metafictional qualities of the dynamic audio-only medium of podcasts are absent from static visual mediums.},
  langid = {english},
  keywords = {_actual play,_canon,_collaborative,_dnd_game,_MEDIA,_peer reviewed,_PSYCHOLOGY,_SOCIOLOGY,_TTRPG,=cleaned,audio,Canon,collaboration,digital storytelling,Dungeons & Dragons,fourth   wall,metafiction,new media,podcast fiction,radio,serial,tabletop roleplaying},
  annotation = {WOS:000565905000001}
}

@thesis{wyndow_subculture_2022,
  type = {MA Sociology},
  title = {The Subculture of the {{Lurker}} : Participatory Non-Participation in Digital Subculture.},
  shorttitle = {The Subculture of the {{Lurker}}},
  author = {Wyndow, Jackson},
  date = {2022},
  institution = {University of Canterbury},
  location = {Christchurch},
  doi = {10.26021/13213},
  url = {https://ir.canterbury.ac.nz/handle/10092/104116},
  urldate = {2022-08-12},
  abstract = {This thesis develops a new theorisation of subculture in digital gaming culture. I propose that  online streaming services and online interaction about games have created an under-theorized  subculture of those who engage in the gaming subculture without playing games themselves.    Existing theorisations of subculture in relation to online gaming tend to limit their accounts to  those who are actively engaged in online gaming. However, based on a grounded analysis of  ethnographic observation, surveys, and interviews with the fans for web series Critical Role,  where famous voice actors play Dungeons \& Dragons, I found that reasons for and forms of  engagement, include what I have called participatory non-participation. This form of  engagement, while not incorporated into current theorisations of digital subculture, clearly  shares key elements of subculture activity that include a sense of belonging, requirements for  group membership, shared history and symbols. As existing theories are based solely on active  game playing that excludes the crucial element of digital participatory non-participation, it is  appropriate to extend current theorisations to resolve this lacunae and is offered here as a  distinctly new theory of digital subculture: the subculture of the lurker.},
  langid = {english},
  pagetotal = {134},
  keywords = {_actual play,_belonging,_dnd_game,_EMOTIONS,_fandom,_MEDIA,_SOCIOLOGY,_TTRPG,=cleaned}
}

@thesis{wyrostkiewicz_tabletop_2021,
  type = {Engineer's thesis},
  title = {Tabletop {{Role-Playing Games Simulator}} with {{Procedurally Generated 2D Dungeon Levels}}},
  author = {Wyrostkiewicz, Aleksandra},
  date = {2021},
  institution = {Warsaw University of Technology},
  location = {Warsaw},
  url = {https://repo.pw.edu.pl/info/bachelor/WUTec1582bf9dad4639908e1acd73679259/},
  urldate = {2021-10-03},
  abstract = {Tabletop Role-Playing Game is a form of an asymmetrical role-playing game, where participants incarnate in the characters and act out their role, describing their actions through speech. While voice can be enough of a tool for the simplest games, additional ones are usually used for visualization of encounters or for dungeon crawling. This thesis demonstrates a Virtual Tabletop program, which is a game helper tool for preparing and playing tabletop role-playing games. It consists of a procedural generator of dungeon levels, a level editor, and a level play-through simulator. The procedural generation is using modified Space Partitioning Algorithm and Cellular Automaton for two types of dungeon levels, mansion-like and cavern-like respectively. This work also includes an analysis of the most common procedural map generation methods and their usage in game development, as procedural content generation is an increasingly popular way of cutting down costs and development time, while keeping the content original and interesting.},
  langid = {english},
  keywords = {_procedural storytelling,_TTRPG,=cleaned}
}

