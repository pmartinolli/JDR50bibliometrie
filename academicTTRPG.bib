@incollection{fritzon_role-playing_2006,
  title = {With {{Role-Playing}} in {{Mind}}. {{A Cognitive Account}} of {{Decoupled Reality}}},
  booktitle = {Role, {{Play}}, {{Art}}. {{Collected Experiences}} of {{Role-Playing}}},
  author = {Lieberoth, Andreas},
