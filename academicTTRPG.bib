@inproceedings{ellis_computers_2017,
  title = {Computers {{Play Chess}}, {{Computers Play Go}}; {{Humans Play Dungeons}} \& {{Dragons}}},
  booktitle = {{{IEEE Intelligent Systems}}},
  author = {Ellis, Simon and Hendler, James},
  date = {2017},
  volume = {32},
  pages = {31--34},
  doi = {10.1109/mis.2017.3121545},
  url = {http://ieeexplore.ieee.org/document/8012338/},
  abstract = {Ten years ago, Go playing was stated as a challenge for AI researchers with an agenda of looking at pattern-recognition and temporal reasoning, as well as game-playing in general. With the success of Go playing programs, it is clear great strides have been made in these areas. In language and broad reasoning, the Watson program was able to beat human experts in Jeopardy! In this article we look at some of the successes of current cognitive architectures on these games. We then explore new challenges in games that will require pushing new agendas in cognitive computing, particularly using Dungeons and Dragons (TM) as an example.},
