@thesis{premont_revoir_2022,
  type = {MA Études cinématographiques},
  title = {Revoir l’histoire du jeu de rôle numérique comme genre vidéoludique : bilan historiographique et enquête historique dans 4 magazines de jeux en Amérique du Nord, 1981-2006},
  shorttitle = {Revoir l’histoire du jeu de rôle numérique comme genre vidéoludique},
