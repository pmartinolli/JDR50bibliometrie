@incollection{hammer_text-based_2024,
  title = {Text-{{Based Role-Playing Games}}},
  booktitle = {The {{Routledge Handbook}} of {{Role-Playing Game Studies}}},
  author = {Hammer, Jessica and Czege, Paul},
  date = {2024},
  publisher = {Routledge},
  abstract = {This chapter describes text-based role-playing games in which players participate and interact via the medium of text. Text-based role-playing games involve a wide range of different designs and technologies to support play. For example, early text-based role-playing games were typically play-by-mail (PBM). Players wrote out their game moves and mailed them to a coordinator, who collated player contributions and responded in writing to the group. In contrast, contemporary text-based role-playing games focus on the process of writing, whether individually or collaboratively. This chapter presents two contemporary text-based role-playing game genres: the individual practice of journaling games and the collaborative practice of online freeform role-play. While these forms appear divergent on the surface, in effect they have three key common features: text (writing is a key part of play), trace (playing creates a physical or digital artifact), and temporality (they are typically played slowly and asynchronously). These features are important for understanding text-based role-playing games more broadly.},
  isbn = {978-1-00-329804-5},
  langid = {english},
  pagetotal = {14},
  keywords = {_CYOA,_DESIGN,_literature,_OTHER GAMES,=cleaned}
}

