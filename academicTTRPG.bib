@incollection{leavenworth_developing_2014,
  title = {The {{Developing Storyworld}} of {{H}}. {{P}}. {{Lovecraft}}},
  booktitle = {Storyworlds across {{Media}} : {{Toward}} a {{Media-Conscious Narratology}}},
