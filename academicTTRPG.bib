@inproceedings{stanfill_they_2024,
  title = {They {{Can}}'t {{Play}} with {{Us}}: {{Pronoun Controversies}} and the {{Reinforcement}} of {{Heteronormativity}} in {{Games}}},
  shorttitle = {They {{Can}}'t {{Play}} with {{Us}}},
  booktitle = {Proceedings of the 19th {{International Conference}} on the {{Foundations}} of {{Digital Games}}},
