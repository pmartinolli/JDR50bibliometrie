  title = {Imaginative {{Role-Playing}} as a {{Medium}} for {{Moral Development}}: {{Dungeons}} \& {{Dragons Provides Moral Training}}},
  shorttitle = {Imaginative {{Role-Playing}} as a {{Medium}} for {{Moral Development}}},
  author = {Wright, Jennifer Cole and Weissglass, Daniel E. and Casey, Vanessa},
