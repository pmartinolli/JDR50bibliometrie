@article{leclaire_live_2020,
  title = {Live {{Action Role-Playing}}: {{Transcending}} the {{Magic Circle}} through {{Play}} in {{Dagorhir}}},
  author = {LeClaire, Matthew M.},
  date = {2020},
  journaltitle = {International Journal of Role-Playing},
  number = {10},
  pages = {56--69},
  url = {http://ijrp.subcultures.nl/wp-content/uploads/2020/11/IJRP-10-Live-Action-Role-playing-Transcending-the-Magic-Circle.pdf},
  abstract = {Individuals interact with one another and develop themselves in accordance with these interactions. One group within this complex system is live action role-players (larpers). Larps are a variant of play that combine the “role-playing” of games, such as Dungeons \& Dragons, with the “live action” aspects of sports. Using symbolic interactionism as its paradigmatic orientation, this study examines how the magic circle is reinforced and challenged through the development of ingame characters and their effect on the out-of-game self. The respondents of this ethnography participate in a larp called Dagorhir (1977-), which puts an emphasis on live action combat rather than role-play. Participants are part of the Las Vegas realm of Dagorhir, Barad’Dun. Players view Dagorhir as a full contact sport mixed with martial arts; role-playing and character development is encouraged, but not necessary. I observed, interviewed, and participated with this group over the period of six months. Even in a combat-oriented larp like Dagorhir, players get to choose names, weapons, clothing, fighting techniques, and other ways to develop their in-game characters; this ability establishes a connection between leisure larps and edu-larps.},
  langid = {english},
  keywords = {_dnd_game,_LARP,_peer reviewed,_SOCIOLOGY,=cleaned},
  annotation = {QID: Q112208861}
}

